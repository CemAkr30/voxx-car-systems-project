# Production Environment Variables
# Copy this file to .env.prod and update values

# Docker Hub Configuration
DOCKERHUB_USERNAME=your-dockerhub-username
DOCKERHUB_TOKEN=your-dockerhub-token

# Kafka Configuration
KAFKA_HOST_NAME=your-server-ip

# Spring Boot Configuration
SPRING_PROFILES_ACTIVE=prod

# Database Configuration
POSTGRES_USER=username
POSTGRES_PASSWORD=password
POSTGRES_DB=voxx_car_systems

# Keycloak Configuration
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin

# Couchbase Configuration
CB_ADMIN_USERNAME=Administrator
CB_ADMIN_PASSWORD=password
CB_BUCKET_NAME=car-bucket
CB_USER=caruser
CB_USER_PASSWORD=caruserpass

# SSL Configuration
SSL_CERT_PATH=/etc/letsencrypt/live/voxxcarsystems.online/fullchain.pem
SSL_KEY_PATH=/etc/letsencrypt/live/voxxcarsystems.online/privkey.pem
